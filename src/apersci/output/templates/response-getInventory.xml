<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:v4="http://v4.core.coexprivate.api.shopatron.com">
   <soapenv:Header/>
   <soapenv:Body>
       <v4:getInventoryResponse>{{range .Return}}
         <v4:getInventoryReturn>
            <v4:LocationName>{{.LocationName}}</v4:LocationName>
            <v4:OnHand>{{.OnHand}}</v4:OnHand>
            <v4:Available>{{.Available}}</v4:Available>
            <v4:PartNumber>{{.PartNumber}}</v4:PartNumber>
            <v4:UPC>{{.UPC}}</v4:UPC>
            <v4:LTD>{{.LTD}}</v4:LTD>
            <v4:SafetyStock>{{.SafetyStock}}</v4:SafetyStock>
            <v4:CountryCode>{{.CountryCode}}</v4:CountryCode>
            <v4:Distance>{{.Distance}}</v4:Distance>
        </v4:getInventoryReturn>{{end}}
      </v4:getInventoryResponse>
   </soapenv:Body>
</soapenv:Envelope>
