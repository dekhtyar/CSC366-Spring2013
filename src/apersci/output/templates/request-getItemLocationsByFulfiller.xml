<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:v4="http://v4.core.coexprivate.api.shopatron.com">
   <soapenv:Header/>
   <soapenv:Body>
      <v4:getItemLocationsByFulfiller>
         <v4:request>
             <v4:FulfillerIDs>{{range .FulfillerIDs}}
                 <v4:items>{{.items}}</v4:items>{{end}}
            </v4:FulfillerIDs>
            <v4:LocationID>{{.LocationID}}</v4:LocationID>
            <v4:PostalCode>{{.PostalCode}}</v4:PostalCode>
            <v4:PartNumber>{{.PartNumber}}</v4:PartNumber>
            <v4:UPC>{{.UPC}}</v4:UPC>
         </v4:request>
      </v4:getItemLocationsByFulfiller>
   </soapenv:Body>
</soapenv:Envelope>
